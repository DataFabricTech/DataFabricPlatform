{
  "$id": "https://mobigen.com/schema/api/data/createFolder.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CreateFolderRequest",
  "description": "Create Folder Model entity request",
  "type": "object",
  "javaType": "com.mobigen.vdap.schema.api.data.CreateFolder",
  "properties": {
    "name": {
      "description": "Name that identifies this folder model.",
      "$ref": "../../type/basic.json#/definitions/entityName"
    },
    "displayName": {
      "description": "Display Name that identifies this folder model.",
      "type": "string"
    },
    "description": {
      "description": "Description of the folder instance.",
      "$ref": "../../type/basic.json#/definitions/markdown"
    },
    "service": {
      "description": "Link to the storage service where this folder is hosted in.",
      "$ref": "../../type/basic.json#/definitions/fullyQualifiedEntityName"
    },
    "parent": {
      "description": "Link to the parent folder under which this entity sits.",
      "$ref": "../../type/entityReference.json"
    },
    "numberOfObjects": {
      "description": "The number of objects/files this folder has.",
      "type": "number",
      "default": null
    },
    "size": {
      "description": "The total size in KB this folder has.",
      "type": "number",
      "default": null
    },
    "owners": {
      "description": "Owner of this database",
      "$ref": "../../type/entityReferenceList.json",
      "default": null
    },
    "tags": {
      "description": "Tags for this folder Model",
      "type": "array",
      "items": {
        "$ref": "../../type/tagLabel.json"
      },
      "default": null
    },
    "sourceUrl": {
      "description": "Source URL of folder.",
      "$ref": "../../type/basic.json#/definitions/sourceUrl"
    },
    "fullPath": {
      "description": "Full path of the data.",
      "type": "string"
    },
    "lifeCycle": {
      "description": "Life Cycle of the entity",
      "$ref": "../../type/lifeCycle.json"
    }
  },
  "required": ["name", "bucket"],
  "additionalProperties": false
}
