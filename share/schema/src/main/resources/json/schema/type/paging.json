{
  "$id": "https://mobigen.com/schema/type/paging.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Paging",
  "description": "Type used for cursor based pagination information in GET list responses.",
  "type": "object",
  "javaType": "com.mobigen.vdap.schema.type.Paging",
  "properties": {
    "page": {
      "description": "Select Page Number",
      "type": "integer",
      "default": null
    },
    "offset": {
      "description": "Offset used in case of offset based pagination.",
      "type": "integer",
      "default": null
    },
    "size": {
      "description": "Page Size",
      "type": "integer",
      "default": null
    },
    "limit": {
      "description": "Limit used in case of offset based pagination.",
      "type": "integer",
      "default": null
    },
    "total_elements": {
      "description": "Total Elements Count",
      "type": "integer"
    },
    "total_pages": {
      "description": "Total Page Count",
      "type": "integer"
    }
  },
  "required": [],
  "additionalProperties": false
}